{
  "eslint.enable": true,
  "editor.formatOnSave": true,
  "eslint.options": {
    "extensions": [
      ".ts",
      ".mts",
      ".js",
      ".mjs"
    ]
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.tabSize": 2,
  "files.eol": "\n",
  "git.ignoreLimitWarning": true
}