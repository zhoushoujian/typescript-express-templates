{
  "name": "typescript-express-templates",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@shuyun-ep-team/eslint-config": "^1.3.2",
    "@types/express": "4.17.0",
    "@types/node": "^14.14.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "3.5.1"
  },
  "scripts": {
    "checkcode": "eslint -c .eslintrc \"**/*.{js,ts}\"",
    "dev": "ts-node-dev ./server.ts",
    "start": "ts-node ./server.ts",
    "build": "tsc",
    "deploy": "cross-env NODE_ENV=production node ./dist/server.js",
    "db": "mongod --dbpath db --port=27017 --bind_ip 127.0.0.1 --noauth",
    "redis": "redis-server"
  },
  "dependencies": {
    "beauty-logger": "^2.0.5",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "mongodb": "^3.1.10",
    "redis": "^2.8.0",
    "ts-node": "^9.0.0",
    "uuid": "^8.3.1"
  },
  "repository": "https://github.com/zhoushoujian/typescript-express-templates",
  "ts-node": {
    "transpileOnly": true
  }
}
