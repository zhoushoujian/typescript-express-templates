{
  "name": "typescript-express-templates",
  "version": "1.1.0",
  "scripts": {
    "checkcode": "eslint -c .eslintrc.js \"./src/**/*.ts\" && tsc --noEmit",
    "dev": "ts-node-dev -r esm -r tsconfig-paths/register ./src/server.ts",
    "start": "npx ts-node -r esm -r tsconfig-paths/register src/server.ts",
    "deploy": "npm run start",
    "prepare": "husky install"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "express": "^4.21.2",
    "parameter": "^3.6.0"
  },
  "devDependencies": {
    "@szhou/eslint-config": "^1.4.10",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.0",
    "@types/node": "^18.11.9",
    "@types/parameter": "^3.6.2",
    "cross-env": "^7.0.3",
    "esm": "^3.2.25",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "5.3.3"
  },
  "repository": "https://github.com/zhoushoujian/typescript-express-templates",
  "ts-node": {
    "transpileOnly": true
  }
}